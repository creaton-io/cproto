{
  "name": "@cproto/api",
  "version": "0.14.0",
  "license": "MIT",
  "description": "Client library for atproto and Bluesky, modified for Creaton",
  "keywords": [
    "atproto",
    "bluesky",
    "api"
  ],
  "homepage": "https://creaton.social",
  "repository": {
    "type": "git",
