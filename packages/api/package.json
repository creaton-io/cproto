    "multiformats": "^9.9.0",
    "tlds": "^1.234.0"
  },
  "devDependencies": {
    "@atproto/lex-cli": "workspace:^",
    "get-port": "^6.1.2",
    "jest": "^28.1.2",
    "prettier": "^3.2.5"
  }
